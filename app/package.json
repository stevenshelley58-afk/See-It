{
  "name": "see-it-app",
  "version": "1.0.29",
  "private": true,
  "scripts": {
    "build": "remix vite:build",
    "dev": "shopify app dev",
    "config:link": "shopify app config link",
    "app:generate": "shopify app generate",
    "predeploy": "node scripts/inject-version.js",
    "deploy": "shopify app deploy",
    "config:use": "shopify app config use",
    "env": "shopify app env",
    "start": "remix-serve ./build/server/index.js",
    "docker-start": "node scripts/railway-migrate.mjs && npm run generate && npm run start",
    "generate": "prisma generate",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate-statuses": "node scripts/migrate-statuses.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "shopify": "shopify",
    "prisma": "prisma",
    "set-unlimited": "node scripts/set-unlimited-credits.js",
    "graphql-codegen": "graphql-codegen",
    "vite": "vite",
    "monitor:prune": "curl -X GET http://localhost:3000/cron/monitor-prune -H 'Authorization: Bearer ${CRON_SECRET}'",
    "seed:prompts": "npx tsx prisma/seed-prompts.ts"
  },
  "type": "module",
  "engines": {
    "node": "^18.20 || ^20.10 || >=21.0.0"
  },
  "dependencies": {

    "@types/node": "^22.2.0",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^10.0.1",
    "pg": "^8.16.3",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^6.2.2",
